@using Tournamentz.BL.Queries
@model List<TeamQueries.All>

@{
    ViewBag.Title = "Svi timovi";
}

<h2>@ViewBag.Title</h2>

<p>Ovo su svi timovi dostupni u sustavu!</p>

<table class="table table-bordered table-striped table-hover table-responsive">
    <tr>
        <th>Naziv tima</th>
    </tr>
    @foreach (TeamQueries.All team in Model)
    {
        <tr>
            <td>@team.Title</td>
        </tr>
    }
</table>